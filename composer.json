{
    "name": "ibekzod/microcrud",
    "description": "CRUD Structure for backed developers! ",
    "type": "library",
    "keywords": [
        "ibekzod",
        "microcrud",
        "laravel",
        "crud"
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database":   "~5.2.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/filesystem": "~5.2.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support":    "~5.2.0|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.13",
        "mockery/mockery": "^1.4.2",
        "vlucas/phpdotenv": "^4.2.2|^5.4.1",
        "php-coveralls/php-coveralls": "^2.5.2",
        "laravel/legacy-factories": "^1.3.0",
        "doctrine/dbal": "^2.11|^3.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Microcrud\\": "src/"
        }
    },
    "authors": [
        {
            "name": "iBekzod",
            "email": "erkinovbegzod.45@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "prod": [
            "composer install --prefer-dist --no-interaction --no-scripts --no-progress --no-ansi --no-dev",
            "composer dump-autoload"
        ],
        "test": [
            "composer install --prefer-dist --no-interaction --no-scripts --no-progress --no-ansi",
            "composer dump-autoload"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Microcrud\\MicrocrudServiceProvider"
            ]
        }
    }
}