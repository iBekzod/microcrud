{
    "name": "ibekzod/microcrud",
    "description": "CRUD package for Laravel - Service-Repository pattern with caching, queues, and dynamic filtering",
    "type": "library",
    "keywords": [
        "laravel",
        "crud",
        "microcrud",
        "service-repository",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "iBekzod",
            "email": "erkinovbegzod.45@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Microcrud\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Microcrud\\MicrocrudServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
